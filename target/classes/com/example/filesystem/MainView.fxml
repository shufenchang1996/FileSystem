<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.filesystem.Controller.Controller"
            prefHeight="800.0" prefWidth="800.0">

    <TextField fx:id="searchBox" layoutX="50.0" layoutY="25.0" prefHeight="26.0" prefWidth="207.0" />
    <Button fx:id="searchButton" layoutX="300.0" layoutY="25.0" mnemonicParsing="false" onAction="#onButtonClick" text="search" />
    <TextArea fx:id="resultBlock" layoutX="50.0" layoutY="75.0" prefHeight="500.0" prefWidth="550.0" />

</AnchorPane>
